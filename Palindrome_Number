package pratice;
import java.util.Scanner;
public class Palindrome {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.print("Enter a Number to Check Weather the given number is Palindrome or Not : ");
	int a= sc.nextInt();
	int rev=0;
	int org=a;
	while(a!=0) {
		rev=rev*10+a%10;
		a=a/10;
	}
if(rev==org) {
	System.out.print("The Given Number is a Palindrome Number!!.."+org +"..."+rev);
}else {
	System.out.print("The Given Number is Not a Palindrome Number!!.."+org +"..."+rev);
}
}
}
